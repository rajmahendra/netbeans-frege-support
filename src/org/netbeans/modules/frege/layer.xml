<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="frege">
                <attr name="displayName" bundlevalue="org.netbeans.modules.frege.editor.Bundle#Editors/text/frege"/>
                <file name="language.instance">
                    <attr name="instanceCreate" methodvalue="org.netbeans.modules.frege.editor.FregeTokenId.getLanguage"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.api.lexer.Language"/>
                </file> 
                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="FontAndColors.xml">
                                <attr name="displayName" bundlevalue="org.netbeans.modules.frege.editor.Bundle#Editors/text/frege/FontsColors/NetBeans/Defaults/FontAndColors.xml"/>
                            </file>
                        </folder>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="frege" url="Frege.fr"/>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="FregeResolver.xml" url="FregeResolver.xml">
                <attr name="displayName" bundlevalue="org.netbeans.modules.frege.Bundle#Services/MIMEResolver/FregeResolver.xml"/>
            </file>
        </folder>
    </folder>
</filesystem>
